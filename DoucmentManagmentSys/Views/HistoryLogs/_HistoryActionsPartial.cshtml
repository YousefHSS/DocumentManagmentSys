
@model IEnumerable<HistoryAction>
@foreach (var item in Model)
{
<tr>
        <td>
            @Html.DisplayFor(modelItem => item.Action)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedAt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>


    </tr>
    
}

