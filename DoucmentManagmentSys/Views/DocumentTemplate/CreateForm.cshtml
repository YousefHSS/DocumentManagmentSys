@model IEnumerable<string>
@* a dropdown that on chnages displays a new form*@
@{
    ViewData["Title"] = "Create Document";
}
<form method="post" asp-action="GetCreationForm">
    <select name="TemplateTitle" id="TemplateTitle">
    @foreach (var item in Model)
    {
        <option value="@item">@item</option>
    }
</select>
<input type="submit" value="Submit">
</form>



